{
  "name": "hackboxgames_server",
  "version": "0.0.1",
  "description": "HackboxGames",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon",
    "build:ts": "tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "npm run build:ts && npm run tslint",
    "serve": "node -r dotenv/config build/server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AylaJK/HackboxGames"
  },
  "author": "Austin W. & Kayleigh K.",
  "license": "ISC",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "moment": "^2.22.1",
    "mongoose": "^5.0.17",
    "passport": "^0.4.0",
    "passport-discord": "^0.1.3",
    "passport-facebook": "^2.1.1",
    "passport-local": "^1.0.0",
    "passport-remember-me-extended": "0.0.3",
    "socket.io": "^2.1.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/body-parser": "^1.17.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.11.1",
    "@types/express-session": "^1.15.8",
    "@types/mongodb": "^3.0.17",
    "@types/mongoose": "^5.0.11",
    "@types/node": "^8.10.11",
    "@types/passport": "^0.4.5",
    "@types/passport-discord": "^0.1.2",
    "@types/passport-facebook": "^2.1.8",
    "@types/passport-local": "^1.0.33",
    "@types/socket.io": "^1.4.33",
    "@types/uuid": "^3.4.3",
    "nodemon": "^1.17.3",
    "ts-node": "^6.0.2",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ignore": [
      "client/*"
    ],
    "ext": "js,jsx,ts,tsx",
    "exec": "ts-node -r dotenv/config src/server.ts"
  }
}
